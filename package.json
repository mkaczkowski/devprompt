{
  "name": "devprompt",
  "version": "1.0.0",
  "description": "A prompt management tool that parses markdown into sections for selective copying",
  "keywords": [
    "prompt",
    "markdown",
    "react",
    "typescript",
    "ai",
    "llm"
  ],
  "author": "DevPrompt Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkaczkowski/webapp-client.git"
  },
  "bugs": {
    "url": "https://github.com/mkaczkowski/webapp-client/issues"
  },
  "homepage": "https://github.com/mkaczkowski/webapp-client#readme",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:e2e": "vite --mode e2e --host 127.0.0.1",
    "build": "lingui extract --clean && lingui compile && tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test --project=desktop",
    "e2e:mobile": "playwright test --project=mobile",
    "e2e:all": "playwright test",
    "e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "db:types": "dotenv -- supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:studio": "supabase studio",
    "deploy:preview": "netlify deploy --build",
    "deploy:prod": "netlify deploy --build --prod"
  },
  "dependencies": {
    "@clerk/react-router": "^2.3.7",
    "@clerk/themes": "^2.4.46",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource-variable/inter": "^5.2.8",
    "@lingui/core": "^5.7.0",
    "@lingui/react": "^5.7.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/react": "^10.32.1",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.562.0",
    "radix-ui": "^1.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hotkeys-hook": "^4.6.2",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^4.2.0",
    "react-router": "^7.11.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@clerk/testing": "^1.13.26",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.28.0",
    "@lingui/babel-plugin-lingui-macro": "^5.7.0",
    "@lingui/cli": "^5.7.0",
    "@playwright/test": "^1.58.2",
    "@react-spa-scaffold/eslint-config": "^1.1.0",
    "@react-spa-scaffold/prettier-config": "^1.0.0",
    "@react-spa-scaffold/tsconfig": "^1.0.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^25.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-plugin-macros": "^3.1.0",
    "commitlint": "^20.2.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.1.0",
    "msw": "^2.12.10",
    "netlify-cli": "^23.13.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-grab": "^0.0.97",
    "shadcn": "^3.6.2",
    "supabase": "^2.70.5",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.33.0",
    "vite": "^7.0.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
